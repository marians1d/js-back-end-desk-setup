<h1>Edit Desk Setup</h1>
<div class="form">
  {{#with setup}}
  <form action="/edit/{{id}}" method="Post">
    <label for="name">Name</label>
    <input type="text" id="name" name="name" value="{{name}}">
    <label for="name">Description</label>
    <textarea id="name" name="description">{{description}}</textarea>
    <label for="image">ImageUrl</label>
    <input type="text" id="image" name="imageUrl" value="{{imageUrl}}">
    <label for="price">Price Estimate</label>
    <input id="price" name="price" type="number" value="{{price}}">
    <input type="submit" value="Save">
  </form>
  {{/with}}
</div>